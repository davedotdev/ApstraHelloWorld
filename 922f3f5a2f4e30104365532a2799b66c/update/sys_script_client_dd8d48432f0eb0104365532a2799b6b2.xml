	bp.getXMLAnswer(function(answer){
		jslog("Inside");
		var items = JSON.parse(answer).items;
		jslog(items);
		for (var index = 0; index < items.length; ++index) {
				g_form.addOption(
					'blueprint_name',
					items[index].id,
					items[index].label
				);
		}
		g_form.setDisabled('blueprint_name', false);
	});
}]]></script>
